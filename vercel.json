{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/logic/wsgi.py"
    }
  ],
  "builds": [
    {
      "src": "fuzzy/package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist" 
      }
    },
    {
      "src": "logic/wsgi.py", 
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ]
}